{
    "name": "visubezier",
    "displayName": "VisuBezier",
    "description": "Provides a preview when hovering CSS easing functions in VS Code.",
    "version": "1.0.0",
    "publisher": "chriskirknielsen",
    "icon": "visubezier.png",
    "engines": {
        "vscode": "^1.26.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/chriskirknielsen/visubezier.git"
    },
    "keywords": [
        "css",
        "cubic",
        "bezier",
        "preview"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "VisuBezier configuration",
            "properties": {
                "visubezier.defaulteasingfunction": {
                    "default": "linear",
                    "description": "Default easing function to be compared with",
                    "type": "string"
                },
                "visubezier.defaultduration": {
                    "default": "1s",
                    "description": "Default duration for each comparison",
                    "type": "string"
                }
            }
        }
    },
    "capabilities": {
        "hoverProvider": "true"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
